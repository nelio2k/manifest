{
    "product": "couchbase-lite-java",
    "manifests": {
        "couchbase-lite-java/lithium.xml": {
            "release": "3.0.0",
            "release_name": "Couchbase Lite Java - Lithium 3.0.0",
            "production": true,
            "interval": 60,
            "restricted": false,
            "start_build": 1,
            "keep_git": true,
            "trigger_blackduck": true,
            "do-build": true,
            "module_projects": [ "couchbase-lite-java-ee-root" ],
            "jenkins_job": "couchbase-lite-java"
        },
        "couchbase-lite-java/hydrogen.xml": {
            "release": "2.8.0",
            "release_name": "Couchbase Lite Java - Hydrogen 2.8.0",
            "production": true,
            "interval": 1440,
            "restricted": false,
            "start_build": 1,
            "keep_git": true,
            "trigger_blackduck": true,
            "do-build": false,
            "module_projects": [ "product-texts", "couchbase-lite-java-ee-root", "couchbase-lite-java-common", "couchbase-lite-java-ce", "couchbase-lite-core-EE", "couchbase-lite-core" ],
            "jenkins_job": "couchbase-lite-java"
        },
        "couchbase-lite-java/mercury.xml": {
            "release": "2.7.1",
            "release_name": "Couchbase Lite Java - Mercury 2.7.1",
            "production": true,
            "interval": 1440,
            "restricted": false,
            "start_build": 1,
            "keep_git": true,
            "do-build": false
        }
    }
}

